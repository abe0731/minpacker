<p id="notice"><%= notice %></p>

<h1>Airports</h1>

<table>
  <thead>
    <tr>
      <th>Name1</th>
      <th>Name2</th>
      <th>Name3</th>
      <th>Prefecture</th>
      <th>City</th>
      <th>Iata cd</th>
      <th>Map</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airports.each do |airport| %>
      <tr>
        <td><%= airport.name1 %></td>
        <td><%= airport.name2 %></td>
        <td><%= airport.name3 %></td>
        <td><%= airport.prefecture %></td>
        <td><%= airport.city %></td>
        <td><%= airport.iata_cd %></td>
        <td><%= airport.map %></td>
        <td><%= link_to 'Show', airport %></td>
        <td><%= link_to 'Edit', edit_airport_path(airport) %></td>
        <td><%= link_to 'Destroy', airport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Airport', new_airport_path %>
<br><br><%= link_to "Admin","/admin/index" %>
