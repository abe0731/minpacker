<p id="notice"><%= notice %></p>

<h1>Stations</h1>

<table>
  <thead>
    <tr>
      <th>Name1</th>
      <th>Name2</th>
      <th>Name3</th>
      <th>Prefecture</th>
      <th>City</th>
      <th>Train route1</th>
      <th>Train route2</th>
      <th>Map</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stations.each do |station| %>
      <tr>
        <td><%= station.name1 %></td>
        <td><%= station.name2 %></td>
        <td><%= station.name3 %></td>
        <td><%= station.prefecture %></td>
        <td><%= station.city %></td>
        <td><%= station.train_route1 %></td>
        <td><%= station.train_route2 %></td>
        <td><%= station.map %></td>
        <td><%= link_to 'Show', station %></td>
        <td><%= link_to 'Edit', edit_station_path(station) %></td>
        <td><%= link_to 'Destroy', station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station', new_station_path %>
<br><br><%= link_to "Admin","/admin/index" %>
