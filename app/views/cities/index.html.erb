<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<table>
  <thead>
    <tr>
      <th>PrefecturesName1</th>
      <th>id</th>
      <th>Name1</th>
      <th>Name2</th>
      <th>Name3</th>
      <th>Prefecture</th>
      <th>Map</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.prefectures_name1 %></td>
        <td><%= city.id %></td>
        <td><%= city.name1 %></td>
        <td><%= city.name2 %></td>
        <td><%= city.name3 %></td>
        <td><%= city.map %></td>

        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City', new_city_path %>
<br><br><%= link_to "Admin","/admin/index" %>
